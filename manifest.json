{
  "name": "Synology DownloadStation",
  "version": "1.0",
  "description": "For faster adding tasks to Synology Download Station",
  "permissions": ["cookies","declarativeContent","storage","https://*.google.com/","http://*.google.com/","http://192.168.0.25:5000/","https://192.168.0.25:5000/"],
  "background": {
    "scripts": ["js/jquery.js", "js/background.js"],
    "persistent": false
  },
  "browser_action":{
    "default_popup":"html/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
  },
  "options_page": "html/options.html",
  "manifest_version": 2
}